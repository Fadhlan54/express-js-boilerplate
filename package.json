{
  "name": "express-js-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate for express project",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "start": "node -r dotenv/config bin/www",
    "dev": "nodemon -r dotenv/config bin/www",
    "lint": "eslint .",
    "fixlint": "eslint . --fix",
    "test": "jest --setupFiles dotenv/config --coverage --colors --verbose --detectOpenHandles --forceExit --testTimeout=20000"
  },
  "author": "Muhammad Fadhlan Aziz",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.1"
  }
}
